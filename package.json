{
    "name": "aniworld.ts",
    "version": "1.1.2",
    "description": "A TypeScript library for interacting with the Aniworld anime streaming platform or similarly structured services like s.to.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "publish": "rm -rf dist && tsc && npm publish",
        "format": "prettier --write 'src/**/*.{ts,tsx}'",
        "testapp": "bun testapp/index.ts",
        "docs": "typedoc --options typedoc.json",
        "opendocs": "bun run docs && open ./docs/index.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KartoffelChipss/aniworld.ts.git"
    },
    "homepage": "https://github.com/KartoffelChipss/aniworld.ts#readme",
    "bugs": {
        "url": "https://github.com/KartoffelChipss/aniworld.ts/issues"
    },
    "keywords": [
        "aniworld",
        "s.to",
        "anime",
        "typescript",
        "library",
        "streaming",
        "api",
        "scraper",
        "downloader",
        "aniworld.to"
    ],
    "author": {
        "name": "Jan Stra√üburger",
        "email": "contact@strassburger.org",
        "url": "https://strassburger.org/"
    },
    "readme": "README.md",
    "license": "MIT",
    "packageManager": "pnpm@10.15.1",
    "devDependencies": {
        "@types/node": "^24.6.2",
        "prettier": "^3.6.2",
        "typedoc": "^0.28.13",
        "typedoc-github-theme": "^0.3.1"
    },
    "peerDependencies": {
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "cheerio": "^1.1.2",
        "node-cache": "^5.1.2"
    }
}
